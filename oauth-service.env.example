# HubSpot OAuth credentials
CLIENT_ID=your_client_id
CLIENT_SECRET=your_client_secret

# Database connection
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@oauth-db:5432/${POSTGRES_DB}

# Required HubSpot scopes
SCOPES=crm.schemas.companies.write crm.schemas.contacts.write crm.schemas.companies.read crm.schemas.contacts.read

# Database configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=oauth_service
